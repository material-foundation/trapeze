{
  "compilerOptions": {
    "declaration": false,
    "importHelpers": true,
    "jsx": "react",
    "jsxFactory": "createElement",
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "es2018",
      "es2019",
      "dom"
    ],
    "module": "es2020",
    "moduleResolution": "node",
    "newLine": "lf",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "outDir": "dist",
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es6"
  }
}
